{
  "expo": {
    "name": "Imdad",
    "slug": "Imdad",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/background.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#316538"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.group22.Imdad"
    },
    "android": {
      "package": "com.group22.Imdad",
      "permissions": ["CAMERA", "CAMERA_ROLL"],
    }
  }
}